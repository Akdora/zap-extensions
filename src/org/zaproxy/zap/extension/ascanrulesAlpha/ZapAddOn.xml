<zapaddon>
	<name>Active scanner rules (alpha)</name>
	<version>9</version>
	<description>The alpha quality Active Scanner rules</description>
	<author>ZAP Dev Team</author>
	<url>https://code.google.com/p/zap-extensions/wiki/AddOn_ascanrulesAlpha</url>
	<changes>
	<![CDATA[
		Added "HTTPS Content Available via HTTP" scanner. (Issue 1295)<br>
		Added "SQLite" SQL Injection scanner. (Issue 734).<br>
		Added "ShellShock" scanner. (Issue 1347)<br>
		Only show example alerts in dev mode (Issue 1349)
	]]>
	</changes>
	<dependson/>
	<extensions>
		<extension>org.zaproxy.zap.extension.ascanrulesAlpha.ExtensionAscanRulesAlpha</extension>
	</extensions>
	<ascanrules>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.BackupFileDisclosure</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.CrossDomainScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.ExampleSimpleActiveScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.ExampleFileActiveScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.HeartBleedActiveScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.HttpsAsHttpScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.InsecureHTTPMethod</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.RemoteCodeExecutionCVE20121823</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.ShellShockScanner</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.SlackerCookieDetector</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.SourceCodeDisclosureCVE20121823</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.SourceCodeDisclosureFileInclusion</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.SourceCodeDisclosureGit</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.SourceCodeDisclosureSVN</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.SourceCodeDisclosureWEBINF</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesAlpha.SQLInjectionSQLite</ascanrule>
	</ascanrules>
	<pscanrules/>
	<filters/>
	<files>
		<file>xml/example-ascan-file.txt</file>
	</files>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version></not-from-version>
</zapaddon>
