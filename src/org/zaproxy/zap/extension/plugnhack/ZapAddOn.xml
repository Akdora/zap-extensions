<zapaddon>
	<name>Plug-n-Hack Configuration</name>
	<version>5</version>
	<description>Supports the Mozilla Plug-n-Hack standard: https://developer.mozilla.org/en-US/docs/Plug-n-Hack.</description>
	<author>ZAP Dev Team</author>
	<url></url>
	<changes>
	<![CDATA[
	Phase 2 support - intercepting, resending and fuzzing postMessages - TODO<br>
	Updated to use the latest core changes (Issue 609).
	]]>
	</changes>
	<dependson/>
	<extensions>
		<extension>org.zaproxy.zap.extension.plugnhack.ExtensionPlugNHack</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files>
		<file>fuzzers/PlugnHack DOM XSS Oracle</file>
	</files>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version/>
</zapaddon>
