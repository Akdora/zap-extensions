{
  "toolName":"OWASP ZAP",
  "protocolVersion":"0.2",
  "features":{
    "proxy":{
      "PAC":"{{ROOT}}/proxy.pac",
      "CACert":"{{ROOT}}/OTHER/core/other/rootcert/?apikey={{APIKEY}}"
    },
    "commands":{
      "prefix":"zap",
      "manifest":"{{ROOT}}/OTHER/pnh/other/service/?apikey={{APIKEY}}"
    },
    "probe":{
      "transport":"HTTPMessageTransport",
      "endpoint":"{{ROOT}}/JSON/pnh/action/monitor/?apikey={{APIKEY}}&",
      "oracle":"{{ROOT}}/JSON/pnh/action/oracle/?apikey={{APIKEY}}&id=",
      "endpointName":"ZAP"
    }
  }
}