<zapaddon>
	<name>Passive scanner rules (beta)</name>
	<version>7</version>
	<description>The beta quality Passive Scanner rules</description>
	<author>ZAP Dev Team</author>
	<url/>
	<changes>
	<![CDATA[
	Restored the name of "Loosely Scoped Cookie" alert/scanner (Issue 850).<br>
	Restored the name of "Charset Mismatch" alert/scanner (Issue 851).<br>
	Cope with ZAP running from dir other than the one its installed in (Issue 933)
	]]>
	</changes>
	<dependson/>
	<extensions>
		<extension>org.zaproxy.zap.extension.pscanrulesBeta.ExtensionPscanRulesBeta</extension>
	</extensions>
	<ascanrules/>
	<pscanrules>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.CharsetMismatchScanner</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.CookieLooselyScopedScanner</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.CSRFCountermeasures</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.InformationDisclosureDebugErrors</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.InformationDisclosureInURL</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.InformationDisclosureReferrerScanner</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.InformationDisclosureSuspiciousComments</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.InsecureAuthenticationScan</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.InsecureJSFViewStatePassiveScanner</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.ServletParameterPollutionScanner</pscanrule>
		<pscanrule>org.zaproxy.zap.extension.pscanrulesBeta.ViewstateScanner</pscanrule>
	</pscanrules>
	<filters/>
	<files>
		<file>xml/debug-error-messages.txt</file>
		<file>xml/suspicious-comments.txt</file>
		<file>xml/URL-information-disclosure-messages.txt</file>
	</files>
	<not-before-version>2.2.0</not-before-version>
	<not-from-version/>
</zapaddon>
