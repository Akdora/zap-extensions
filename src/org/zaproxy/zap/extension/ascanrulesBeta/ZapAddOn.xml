		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionPostgresql</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SourceCodeDisclosureCVE20121823</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SourceCodeDisclosureSVN</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SourceCodeDisclosureWEBINF</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.UsernameEnumeration</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.XpathInjectionPlugin</ascanrule>                
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.XXEPlugin</ascanrule>
	</ascanrules>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version/>
</zapaddon>
