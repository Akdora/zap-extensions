<zapaddon>
	<name>Script Console</name>
	<version>12</version>
	<description>Supports all JRS 223 scripting languages</description>
	<author>ZAP Dev Team</author>
	<url>https://code.google.com/p/zaproxy/wiki/ScriptConsole</url>
	<changes>
	<![CDATA[
	Disabled "Word Wrap" by default to avoid scrolling performance degradation due to long lines (Issue 1160).
	]]>
	</changes>
	<dependson/>
	<extensions>
		<extension>org.zaproxy.zap.extension.scripts.ExtensionScriptsUI</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version/>
</zapaddon>
