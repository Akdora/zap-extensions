<zapaddon>
	<name>Jython scripting</name>
	<version>1</version>
	<description>Allows Jython to be used for ZAP scripting - templates included</description>
	<author>ZAP Dev Team</author>
	<url></url>
	<changes/>
	<dependson/>
	<extensions>org.zaproxy.zap.extension.jython.ExtensionJython</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files>
		<file>scripts/templates/active/Active default template.py</file>
		<file>scripts/templates/passive/Passive default template.py</file>
		<file>scripts/templates/proxy/Proxy default template.py</file>
		<file>scripts/templates/standalone/Standalone default template.py</file>
		<file>scripts/templates/targeted/Targeted default template.py</file>
	</files>
</zapaddon>
