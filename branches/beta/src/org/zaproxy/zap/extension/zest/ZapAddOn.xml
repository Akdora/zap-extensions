<zapaddon>
	<name>Zest - Scripting Security Tests</name>
	<version>15</version>
	<description>A graphical security scripting language..</description>
	<author>ZAP Dev Team</author>
	<url>https://code.google.com/p/zaproxy/wiki/HelpAddonsZestZest</url>
	<changes>
	<![CDATA[
	ZAP issue 1235: Support client side scripting<br>
	]]>
	</changes>
	<dependson/>
	<extensions>
		<extension>org.zaproxy.zap.extension.zest.ExtensionZest</extension>
	</extensions>
	<filters/>
	<files>
		<file>scripts/templates/active/Active default template.zst</file>
		<file>scripts/templates/authentication/Authentication default template.zst</file>
		<file>scripts/templates/passive/301-302 body.zst</file>
		<file>scripts/templates/passive/Passive default template.zst</file>
		<file>scripts/templates/proxy/Change all POSTs to GETs.zst</file>
		<file>scripts/templates/proxy/Drop requests via URL regexes.zst</file>
		<file>scripts/templates/proxy/Proxy default template.zst</file>
		<file>scripts/templates/proxy/Remove CSP.zst</file>
		<file>scripts/templates/proxy/Replace in request body.zst</file>
		<file>scripts/templates/proxy/Replace in request header.zst</file>
		<file>scripts/templates/proxy/Replace in response body.zst</file>
		<file>scripts/templates/proxy/Replace in response header.zst</file>
		<file>scripts/templates/standalone/Standalone default template.zst</file>
		<file>scripts/templates/targeted/Resend as a GET request.zst</file>
		<file>scripts/templates/targeted/Targeted default template.zst</file>
	</files>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version/>
</zapaddon>
